DROP TABLE IF EXISTS flr;
CREATE TABLE flr (

	hexacle 	text,
	adresse 	text,
	nb_bal		numeric,
	phd_pbd		text,
	parcelle	text,
	zone		text,
	type_habitat	text,
	type_chaussee	text,
	anc_chausse	numeric,
	type_trottoir	text,
	anc_trottoir	text,
	gest_voirie	text,
	nro		text,
	poche		numeric,
	bpi_pdb_pme	text,
	type_adduction	text,
	concessionnaire	text,
	ref_pm		text,
	lr_pm		numeric,
	commentaire	text, 
	analyse_fi	text);
